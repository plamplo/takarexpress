  //- desktop view
  //- v1.0.2
  header.site-header.show-for-large(role='banner')
    .row()
      .small-12.medium-4.large-3.columns(style="margin-top:15px")
        img(src='/images/mockup/takra.png', alt='takra logo')
        h1.show-for-sr Takraexpress
      .small-12.medium-4.large-6.columns(style="padding-top:25px")
        form
          .cellpd
            .input-group
              label.show-for-sr(for='searchitem') Search
              input#searchitem.input-group-field.inputsearch(type='search', placeholder='Find a dealer, model or code...', required='')
              .input-group-button
                button.button(type='submit')
                  span.invisible Go
      .small-12.medium-4.large-3.columns(style="padding-top:25px")
        nav.float-right.celllogin
          a.button.nav-signin.primary-color-bg.bdr4px(href='#', aria-label='Sign Up', role='button') Sign Up
          a.button(href='#', aria-label='Log In', data-open='exampleModal1', role='button') Log In
  #exampleModal1.reveal.bdr4px(data-reveal='')
    .row
      .small-12.medium-12.large-12.columns
        h1.text-center Login
        #error
        label
          | Username
          input(type='text', placeholder='Username', required='' id="username")
        label
          | Password
          input(type='password', placeholder='Password', required='' id="password")
        button.button.float-center.primary-color-bg.bdr4px(onclick="login();") Sign in to your account
        p
          | New here?
          a(href='#') Create an account

